{
  "name": "ai-podcast-clipper-saas",
  "version": "1.0.0",
  "description": "AI-powered podcast clipper that converts full podcasts into viral short-form clips",
  "main": "index.js",
  "scripts": {
    "dev": "cd ai-podcast-clipper-frontend && npm run dev",
    "build": "cd ai-podcast-clipper-frontend && npm run build",
    "start": "cd ai-podcast-clipper-frontend && npm start",
    "setup": "npm run setup:frontend && npm run setup:backend",
    "setup:frontend": "cd ai-podcast-clipper-frontend && npm install",
    "setup:backend": "cd ai-podcast-clipper-backend && pip install -r requirements.txt",
    "inngest-dev": "cd ai-podcast-clipper-frontend && npm run inngest-dev",
    "db:push": "cd ai-podcast-clipper-frontend && npm run db:push",
    "db:studio": "cd ai-podcast-clipper-frontend && npm run db:studio"
  },
  "keywords": [
    "ai",
    "podcast",
    "clipper",
    "saas",
    "nextjs",
    "python",
    "fastapi",
    "stripe",
    "modal"
  ],
  "author": "Andreas Trolle",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Andreaswt/ai-podcast-clipper-saas.git"
  }
}